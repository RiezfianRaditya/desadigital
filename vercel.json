{
  "version": 2,
  "framework": null,
  "builds": [
    {
      "src": "/public/index.php",
      "use": "vercel-php@0.7.1"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/public/index.php"
    }
  ],
  "env": {
    "APP_ENV": "production",
    "APP_DEBUG": "false",
    "LOG_CHANNEL": "stderr",
    "CACHE_DRIVER": "array",
    "APP_URL": "https://desadigital-gilt.vercel.app/",
    "SESSION_DRIVER": "cookie"
  }
}
